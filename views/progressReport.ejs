                                <!-- ng-click="ViewProgressReportSpecific()"  -->
                                <div class="form-container" style="overflow-x: none;" ng-repeat="dates in progressReport">

                                <ul>
                                  <li  style="margin-left:-30px;"
                                  ng-model="formData.progressReport_SpecificDate"
